{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "src/*": ["src/*"],
      "components/*": ["src/components/*"],
      "assets/*": ["src/assets/*"],
      "css/*": ["src/css/*"],
      "layouts/*": ["src/layouts/*"],
      "i18n/*": ["src/i18n/*"],
      "page/s*": ["src/page/s*"],
      "router/*": ["src/router/*"],
      "store/*": ["src/store/*"]
    }
  },
  "exclude": ["dist", ".quasar", "node_modules", ".vscode"],
  "noImplicitAny": false,
  "allowJs": true
}
